<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:int="http://www.springframework.org/schema/integration"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/integration http://www.springframework.org/schema/integration/spring-integration.xsd">


	<int:gateway service-interface="com.example.demo.service.PrinterGateway" default-request-channel="channel1In"/>

	<int:channel id="channel1In">
	</int:channel>

	<bean class="com.example.demo.service.PrintService" id="printService" />

	<bean class="com.example.demo.service.UppercasePrintService" id="uppercasePrintService" />

	<int:service-activator order="1" ref="printService" method="print" input-channel="channel1In" />

	<int:service-activator order="2" ref="uppercasePrintService" method="print" input-channel="channel1In" />

</beans>
